  hrp.CFrame = pos
end)

local Misc = Window:NewTab("Visual")

local Section = Misc:NewSection("Visual Stuff")

Section:NewButton("Remove Npcs", "", function()
  for _, mod in ipairs(workspace:GetDescendants()) do
    if mod:IsA("Model") and mod.Name == "" then
      mod:Destroy()
    end
  end
end)

